mixin deployScripts(list)
	each source in list
		script(src=source)
mixin deployStyles(list)
	each source in list
		link(rel="stylesheet" href=source type="text/css")
mixin makeHeaderAndNav(menu, pageName)
	header
		nav.navbar-nav.navbar-fixed-top.navbar-default
			div.container
				div.navbar-header.navbar-left
					button(type="button" data-toggle="collapse" data-target="#navbar-collapse").navbar-toggle.collapsed
						span.sr-only |Toggle navigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					#navbrand.container
						img(src="../img/Logotop.png" width="150" height="150") 
				#navbar-collapse.collapse.navbar-collapse
					ul.nav.navbar-nav.navbar-right
						each point in menu
							li
								a(href=point.href)=point.name 
					form.navbar-form.navbar-right(role='search')
						.form-group
							input.form-control(type='text', placeholder='Find')
							button.btn.btn-default(type='submit') Submit

mixin addTypicalArticle(headerText, text)
	article
		header
			h1 #{headerText}
		p #{text}
mixin addIcon(icon)
	link(rel='icon', href='/favicon.ico', type='image/x-icon')

